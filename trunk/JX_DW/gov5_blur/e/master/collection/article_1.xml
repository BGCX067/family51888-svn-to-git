<?xml version="1.0" standalone="yes"?>
<Collection>
  <Fields field="title">
    <name>标题</name>
    <type>nvarchar</type>
    <maxlength>200</maxlength>
    <c_rules>&lt;title&gt;(?&lt;content&gt;.+)&lt;/title&gt;</c_rules>
    <n_rules />
    <replace_rules>-pageadmin学校网站模板-Powered by PageAdmin CMS</replace_rules>
  </Fields>
  <Fields field="pa_source">
    <name>来源</name>
    <type>nvarchar</type>
    <maxlength>200</maxlength>
    <c_rules>&amp;nbsp;&amp;nbsp;来源：(?&lt;content&gt;[^&lt;&gt;&amp;]*)&amp;nbsp;&amp;nbsp;</c_rules>
    <n_rules />
    <replace_rules />
  </Fields>
  <Fields field="thedate">
    <name>发布日期</name>
    <type>datetime</type>
    <maxlength>200</maxlength>
    <c_rules>发布时间：(?&lt;content&gt;[^&lt;&gt;&amp;]*)&amp;nbsp;&amp;nbsp;</c_rules>
    <n_rules />
    <replace_rules />
  </Fields>
  <Fields field="content">
    <name>内容</name>
    <type>ntext</type>
    <maxlength>0</maxlength>
    <c_rules>&lt;div id="Infor_Content"&gt;(?&lt;content&gt;.+)&lt;/div&gt;\s*&lt;script type="text/javascript" src="/js/internal_page.js"&gt;&lt;/script&gt;</c_rules>
    <n_rules />
    <replace_rules />
  </Fields>
</Collection>